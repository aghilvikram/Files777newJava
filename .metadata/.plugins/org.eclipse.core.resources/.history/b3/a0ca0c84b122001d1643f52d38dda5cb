package programs;

public class SwappingTwoNumbers {
	public static void m1() {
		int a=100;
		int b=200;
		a=a-b;
		b=a+b;
		a=b-a;
		System.out.println(a);
		System.out.println(b);
	}

	public static void main(String[] args) {
		int a=100;
		int b=200;
		int t=a;
		a=b;
		b=t;
		
		System.out.println(a);
		System.out.println(b);
		m1();

	}

}
