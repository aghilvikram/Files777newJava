package javapac;

public class Strnew {
	
	 static String str="vikram";
    static String rev="";
	

	public static void main(String[] args) {
		
     
     int len = str.length();
     System.out.println(len);
     for(int i=len-1;  i<=0; i--) {
    	 rev=rev+str.charAt(i);
     }
    System.out.println(rev);
     
	}

}
